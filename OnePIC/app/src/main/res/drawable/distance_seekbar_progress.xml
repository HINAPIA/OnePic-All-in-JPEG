<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <!--시크바 배경-->
    <item android:id="@android:id/background">
        <shape android:shape="line">
            <!-- 시크바 선 굵기 : 길이 10dp -->
            <stroke android:width="3dp" android:color="@color/white_85" />
            <!-- 시크바 내용 색상 -->
<!--            <solid android:width="2dp" android:color="@color/white"/>-->
            <!-- 테두리 둥글게 설정 -->
            <corners android:radius="10dp"/>
        </shape>
    </item>

    <!-- 시크바 진행 프로그레스 -->
    <item android:id="@android:id/progress" >
        <clip>
            <shape android:shape="line">
                <!-- 시크바 선 굵기 : 길이 10dp -->
                <!--                <stroke android:width="2dp" android:color="#525252" />-->
                <!-- 시크바 내용 색상 -->
                <stroke android:width="3dp" android:color="@color/dark_purple_85"/>
                <!-- 테두리 둥글게 설정 -->
                <corners android:radius="10dp"/>
            </shape>
        </clip>
    </item>

</layer-list>
